    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django:Runserver",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/Backend/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "justMyCode": false
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "React Debug",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        }
    ]
}